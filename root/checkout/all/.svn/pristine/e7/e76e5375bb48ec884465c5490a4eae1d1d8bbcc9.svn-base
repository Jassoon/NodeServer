.swiper-container {
	width: 95%;
	height: 83%;
	color: #fff;
	margin: 0 2.5%;
	background: #f9f9f9;
	text-align: center;
}
.swiper-slide{
	position: relative;
}
.swiper-slide .title {
  font-style: italic;
  font-size: 42px;
  margin-top: 80px;
  margin-bottom: 0;
  line-height: 45px;
}
.swiper-slide p {
  font-style: italic;
  font-size: 25px;
}
.pagination1,.pagination2,.pagination3,.pagination4,.pagination5 {
  position: absolute;
  left: 0;
  text-align: center;
  bottom:5px;
  width: 100%;
  background: #fff;
}
.swiper-pagination-switch {
  display: inline-block;
  width: 10px;
  height: 10px;
  border-radius: 10px;
  background: #e0e0e0;
  //box-shadow: 0px 1px 2px #555 inset;
  margin: 0 3px;
  cursor: pointer;
}
.swiper-active-switch {
  background: #ff9860;
}