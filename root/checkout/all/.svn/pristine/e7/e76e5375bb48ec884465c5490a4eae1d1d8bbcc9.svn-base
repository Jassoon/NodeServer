//@import 'h5_common.scss';
$url:"../images/";
//$url:"http://192.168.39.130/work/8.26/web/images/";
$bordercolor: #ccc;
$linkcolor: #ccc;
$linkhover: #339933;
@mixin img($p){background:url($url+$p) no-repeat;}
@mixin wh($w , $h){width: $w; height:$h;}
@mixin lt($l , $t){left: $l; top:$t;}
@mixin rb($r , $b){right: $r; bottom:$b;}
@mixin lb($l , $b){left: $l; bottom:$b;}
@mixin rt($r , $t){right: $r; top:$t;}
@mixin bg($img,$rp){background:url($url + $img) $rp}
.overflow_box{-o-text-overflow: ellipsis; text-overflow: ellipsis; overflow:hidden; white-space:nowrap;}
body{}
.clear{clear: both;font-size: 0;}
.doc_select{
	position: relative;	
	//display: none;
	padding-top: 15px;
	.select_con{
		padding: 0 5%;
		.select{
			width: 50%;
			float: left;
			.select_pic{
				position: relative;
				padding: 5%;
				.dab{
				position: absolute;
				@include rb(8%,8%);
				@include wh(30px,30px);
				@include bg("dab_n.png",no-repeat);
				background-size: 100% 100%;
				z-index: 6;
				opacity: 1;
				}
				.dab_y{
					@include bg("dab_y.png",no-repeat);
					background-size: 100% 100%;
				}
			}
			.select_text{
				width: 100%;
				height: 35px;
				line-height: 35px;
				text-align: center;
				font-weight: bold;
				font-size: 12px;
			}
		}
	}
	.s_goat{
		padding-top: 20px;
	}	
}
.doc_show{
	position: relative;	
	background: #ffa56e;
	display: none;
	.con{
		width: 95%;
		margin: 0 2.5%;	
		background: #fff;
		padding-top: 10px;	
		.add_text{
			margin: 20px 2.5%;
			//min-height: 50px;
			position: relative;
			.user_pic{
				width: 15%;		
			}
			.user_input{
				@include wh(78%,100%);
				@include rt(0,0);
				border-radius:10px;
				position: absolute;
				@include bg('user_input.jpg',no-repeat);
				background-size: 100% 100%;
				padding:3px 0 0 4%;
				textarea{
					@include wh(95%,95%);
					color:#999;
					border:none;
					background: none;
					font-size: 12px;
				}
			}
		}
		.add_talk{
			margin: 20px 2.5%;
			//min-height: 50px;
			position: relative;
			.user_pic{
				width: 15%;		
			}
			.user_talk{
				@include wh(80%,100%);
				@include lt(20%,0);
				//border-radius:10px;
				position: absolute;
				.talk_btn{
					@include wh(100%,100%);
					border-radius:5px; 
					//border:1px solid #d1d1d1;
					color:#fff;
					line-height: 3;
					float: left;
				}
				.talk_btn_hou{
					border:1px solid #d1d1d1;
					@include bg("talk_hou_lb.png",no-repeat);
					background-position: 10px center;
					background-size: auto 100%;
					@include wh(80%,100%);
				}
				.talk_btn_fang{
					border:1px solid #d1d1d1;
					@include bg("talk_bf_lb.gif",no-repeat);
					background-position: 10px center;
					background-size: auto 100%;
					@include wh(80%,100%);
				}
				.talk_cz{
					display: none;
					@include wh(19%,100%);
					float: left;
					position: relative;
					.talk_new{
						@include wh(5px,5px);
						background: #ff6364;
						position: absolute;
						@include lt(3px,0);
						border:1px solid #d1d1d1;
						border-radius:30px;
					}
					.talk_time{
						@include wh(20px,20px);
						position: absolute;
						@include lb(3px,0);
						color: #999;
						font-size: 14px;
					}
				}
			}
		}
		.in_line{
			@include wh(90%,0);
			border-top:1px dashed #ccc;
			font-size: 0;
			line-height: 0;
			margin:30px auto 20px auto;

		}
		.eat_text{
			@include wh(100%,30px);
			font-size: 14px;
			line-height: 30px;
			text-align: center;
			span{
				font-weight: bold;
				color:#ffbb1c;
				margin-right: 2px;
			}
		}
		.eat{
			display: none;
		}
		.tab_con_element{
				position: relative;
				background:#f9f9f9;
				width:95%;
				margin:0 2.5%;
				.other_pic{
					position: absolute;
					@include wh(35%,100%);
					@include lt(0,0);
					z-index: 5;
					img{
						position: absolute;
						@include lt(10%,0);
						top:10%;
						height: 80%;
					}
				}
				.other_text{
					position: absolute;
					@include wh(60%,100%);
					@include rt(0,0);
					z-index: 5;
					color: #000;
					.other_text_a{
						position: absolute;
						@include lt(0,20%);
						@include wh(100%,20%);
						border-bottom:1px solid #ccc;
					}
					.other_text_b{
						position: absolute;
						@include lt(0,50%);
						@include wh(100%,20%);
						border-bottom:1px solid #ccc;
					}
					input{
						@include wh(80%,22px);
						border:none;
						background: none;
						font-size: 12px;
						color:#ccc;
					}
				}
			}
		.summit_to{
		padding:10px 0;
	}
	}
	.huawen{
		margin: 0 2.5%;	
	}
	.pop_xy{
		display: none;
		@include wh(100%,100%);
		@include lt(0,0);
		position: fixed;
		background-size: 100%;
		background: rgba(0,0,0,0.8);
		z-index: 50;
	}
}
