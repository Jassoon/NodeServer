	<!-- 操作 -->
	<script>
	var swiperstar = function(){
	  var mySwiper = new Swiper('#swiper-container1',{
	   	pagination: '.pagination1',
	   	loop:true,
	   	onSlideChangeEnd: function(swiper){
     		$(".dab").removeClass('dab_y')
    	}
	  })
	  var mySwiper2 = new Swiper('#swiper-container2',{
	    pagination: '.pagination2',
	   	loop:true,
	   	onSlideChangeEnd: function(swiper){
     		$(".dab").removeClass('dab_y')
    	}
	  })
	  var mySwiper3 = new Swiper('#swiper-container3',{
	    pagination: '.pagination3',
	   	loop:true,
	   	onSlideChangeEnd: function(swiper){
     		$(".dab").removeClass('dab_y')
    	}
	  })
	  var mySwiper4 = new Swiper('#swiper-container4',{
	    pagination: '.pagination4',
	    loop:true,
	    onSlideChangeEnd: function(swiper){
     		$(".dab").removeClass('dab_y')
    	}
	  })
	  var mySwiper5 = new Swiper('#swiper-container5',{
	    //pagination: '.pagination5',
	    //loop:true,
	    onSlideChangeEnd: function(swiper){
     		$(".dab").removeClass('dab_y')
    	}
	  });

	  //Tab切换
	  var tabMap = {
	  		0: mySwiper,
	  		1: mySwiper2,
	  		2: mySwiper3,
	  		3: mySwiper4,
	  		4: mySwiper5
	  	};
	  }

	$("#pic").click(function(){
		api.pic();
	});
	$("#summit").click(function(){
		$(".pop_fx").show();
	});
	$(".pop_fx").click(function(){
		$(this).hide();
	});
	$("#view").delegate(".place_submit","click",function(){
		api.place_submit(this);	
	});
	//Tab切换
	var nowTab = 0,
		nowSty = 0;
	$(".tab_title .tab_title_click").eq(0).addClass("now");
	$(".tab_title .tab_title_click").click(function() {
		nowTab = $(this).index();
		$(".dab").removeClass('dab_y')
		$(this).addClass("now").siblings().removeClass("now");
		$(".tab_con .tab_con_element").eq($(".tab_title .tab_title_click").index(this)).show().siblings(".tab_con_element").hide();
	});

	$(".tab_con_element").click(function(){
		$(".dab").addClass('dab_y')
	})
	$(".select").click(function(){
		nowSty = $(this).index();
		$('.dab').removeClass('dab_y');
		$(this).find('.dab').addClass('dab_y');
	})
	$(".s_goat").click(function() {
		$(".doc_select").hide();
		$(".doc").fadeIn("slow");
		if(nowSty==0){
			swiperstar()
		}else if(nowSty==1){
			$(".doc,.doc_show").css("background","#75bb4b")
			$("#head img").attr("src","images/p_01_dsg.jpg")
			$("#footer img").attr("src","images/p_09_dsg.jpg")
			swiperstar()
		}else if(nowSty==2){
			$(".doc,.doc_show").css("background","#8461de")
			$("#head img").attr("src","images/p_01_gm.jpg")
			$("#footer img").attr("src","images/p_09_gm.jpg")
			swiperstar()
		}else if(nowSty==3){
			$(".doc").css("background","#3cb6ff")
			$("#head img").attr("src","images/p_01_hjy.jpg")
			$("#footer img").attr("src","images/p_09_hjy.jpg")
			swiperstar()
		}

	});
	</script>
	<!-- 逻辑 -->
	<script>
