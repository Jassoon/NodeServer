            body {font-family:Arial,Verdana,Helvetica,"microsoft yahei",sans-serif;min-width:320px; background: #f5f5f5} body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,button,textarea,p,blockquote,th,td,figure {padding:0;margin:0;} fieldset,img{border:0;} address,caption,cite,code,dfn,em,th,var,optgroup {font-style:normal;font-weight:normal;} h1,h2,h3,h4,h5,h6 {font-size:100%;} input,button,textarea,select,optgroup,option {font-family:inherit;font-size:inherit;font-style:inherit;font-weight:inherit;} ol,ul,li {list-style:none;} table {border-collapse:collapse;border-spacing:0;}caption,th {text-align:left;} a,input,button{ -webkit-tap-highlight-color:rgba(0,0,0,0); -webkit-tap-highlight-color:transparent;/* For some Androids */ -webkit-appearance:none; -moz-appearance:none; text-decoration:none;}
            /*定义最外面的容器宽度及样式*/
            .ui-page {
                width: 100%;
                height: auto;
                overflow: hidden;
                position: relative;
                background: #f5f5f5;
				padding-bottom:20px;
            }
            /*定义页面全部图片的大小宽度100%*/
            .ui-page img {
                display: block;
                margin: 0;
                width: 100%;
            }
            /*头像*/
            .ui-head-portrait {
                width: 14.0625%;
                position: absolute;
            }
            .ui-left {
                left: 3.125%;
            }
            .ui-right {
                right: 3.125%;
            }
            /*对话*/
            .ui-div-dialog,
			.ui-div-dialog-text {
                width: 100%;
                position: relative;
				opacity:0;
				/*visibility:block;
				
				display:none;
				-webkit-transition:all ease-in 500ms;
				-moz-transition:all ease-in 500ms;
				-ms-transition:all ease-in 500ms;
				-o-transition:all ease-in 500ms;
				transition:all ease-in 500ms;
				*/
                display: none;
            }
			.ui-div-dialog-animation {
                display: block;
				animation:gonext linear 2000ms;
				animation-fill-mode:forwards;
			}
            .ui-div-dialog-animation2 {
                display: block;
                animation:gonext2 linear 1000ms;
                animation-fill-mode:forwards;
            }
			.ui-div-dialog-animation-text {
                display: block;
				animation:gonexttext linear 2000ms;
				animation-fill-mode:forwards;
			}
			.ui-div-dialog {
				margin-top: 15px;
			}
			.ui-div-dialog-text {
				color:#333333;
				padding: 10px 0;
				text-align:center;
			}
			.ui-div-dialog-text h1{
				font-size: 24px;
				font-weight:normal;
			}
			.ui-div-dialog-text p {
				font-size: 12px;
			}
			@keyframes gonext {
				0%   { margin-top: 15px; opacity:0;}
				30%  { margin-top: 15px; opacity:1;}
                100% { margin-top: 15px; opacity:1;}
			}

            @keyframes gonext2 {
                0%   { margin-top: 15px; opacity:0;}
                100% { margin-top: 15px; opacity:1;}
            }
			@keyframes gonexttext {
				0%   { margin-top: 15px; opacity:0;}
				20%  { margin-top: 15px; opacity:1;}
				80%  { margin-top: 15px; opacity:1;}
				100% { margin-top: 15px; opacity:0; display:none}
			}
            /*设置了超出1024像素的宽度个别样式就以下面的为准*/
            @media only screen and (min-width: 1024px) {
                .ui-page {
                    width: 640px;
                    margin: 0 auto;
                }
            }
        </style>
