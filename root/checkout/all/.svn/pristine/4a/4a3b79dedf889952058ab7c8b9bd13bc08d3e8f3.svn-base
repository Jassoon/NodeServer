    <script type="text/javascript">
		function gonext (animateid,animatecss,animateactions){
			function cb() {
				animateactions(animatecss);
			}
			animateid.addEventListener("webkitAnimationEnd", cb);
			animateid.addEventListener("animationend", cb);
		}
        function animate(animatecss) {
            animatecss.classList.add("ui-div-dialog-animation");
            window.scrollTo(0,document.body.scrollHeight);
            //$('html,body').animate({scrollTop:animateid.offset.top}, 100);
        }
        function animate_2(animatecss) {
            animatecss.classList.add("ui-div-dialog-animation2");
            window.scrollTo(0,document.body.scrollHeight);
            //$('html,body').animate({scrollTop:animateid.offset.top}, 100);
        }
		function animatetext(animatecss) {
			animatecss.classList.add("ui-div-dialog-animation-text");
			
		}
		gonext (animate1, animatetext1, animatetext);
		gonext (animatetext1, animate2, animate);
		gonext (animate2, animatetext2, animatetext);
		gonext (animatetext2, animate3, animate);
		gonext (animate3, animatetext3, animatetext);
		gonext (animatetext3, animate4, animate);
		gonext (animate4, animatetext4, animatetext);
		gonext (animatetext4, animate5, animate);
		gonext (animate5, animate6, animate);
		gonext (animate6, animatetext5, animatetext);
		gonext (animatetext5, animate7, animate);
		gonext (animate7, animate8, animate);
		gonext (animate8, animate9, animate);
		gonext (animate9, animate10, animate);
		gonext (animate10, animate11, animate_2);
		gonext (animate11, animate12, animate_2);
		gonext (animate12, animate13, animate_2);
		gonext (animate13, animate14, animate_2);
		gonext (animate14, animate15, animate_2);
		
		
		function texthide (animatetexts,functiontxt){
			animatetexts.addEventListener("webkitAnimationEnd",functiontxt);
			animatetexts.addEventListener("animationend",functiontxt);
			function functiontxt (){
				animatetexts.style.display = "none";
			}
		}
		texthide (animatetext1,animatetext1);
		texthide (animatetext2,animatetext2);
		texthide (animatetext3,animatetext3);
		texthide (animatetext4,animatetext4);
		texthide (animatetext5,animatetext5);
		
		/*document.getElementById("animate6").addEventListener("webkitAnimationEnd",scrollfun1);
		document.getElementById("animate6").addEventListener("animationend",scrollfun1);
		
		function scrollfun1 (){
			$('html,body').animate({scrollTop:$('#animate6').offset().top}, 800);
		}
		
		document.getElementById("animate12").addEventListener("webkitAnimationEnd",scrollfun2);
		document.getElementById("animate12").addEventListener("animationend",scrollfun2);
		
		function scrollfun2 (){
			$('html,body').animate({scrollTop:$('#animate10').offset().top}, 800);
		}*/

		for (var i=1; i<=9; i++){
			document.getElementById("animate"+i).addEventListener("webkitAnimationStart",audiofun1);
			document.getElementById("animate"+i).addEventListener("animationstart",audiofun1);
		}
		
        function audiofun1 (){
