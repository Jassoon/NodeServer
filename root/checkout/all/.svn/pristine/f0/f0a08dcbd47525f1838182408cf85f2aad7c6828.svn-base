  line-height: 45px; }

.swiper-slide p {
  font-style: italic;
  font-size: 25px; }

.pagination1, .pagination2, .pagination3, .pagination4, .pagination5 {
  position: absolute;
  left: 0;
  text-align: center;
  bottom: 5px;
  width: 100%;
